{
    "name": "gzhegow/orm",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",

    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "Gzhegow\\Orm\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Orm\\Tests\\": "tests/src/"
        }
    },

    "require": {
        "php": "^7.3|^8.0",

        "ext-json": "*",
        "ext-pdo": "*",

        "gzhegow/lib": "~1.0",

        "illuminate/database": "~8.0"
    },

    "config": {
        "optimize-autoloader": true,

        "sort-packages": false
    }
}